{
    "name": "transcript-analysis-system",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:integration": "vitest run tests/integration",
        "test:unit": "vitest run tests/unit",
        "test:coverage": "vitest run --coverage",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "test:with-docker": "npm run docker:up && npm run test:integration && npm run docker:down",
        "test:storage": "vitest run lib/storage --max-concurrency=1 --sequence.concurrent=false"
    },
    "dependencies": {
        "@supabase/supabase-js": "^2.39.0",
        "@vercel/blob": "^0.15.1",
        "install": "^0.13.0",
        "luxon": "^3.5.0",
        "nanoid": "^5.0.3",
        "next": "13.5.4",
        "npm": "^11.2.0",
        "react": "^18",
        "react-dom": "^18"
    },
    "devDependencies": {
        "@tailwindcss/postcss": "^4.0.14",
        "@types/luxon": "^3.4.2",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "@vitest/coverage-v8": "^1.3.1",
        "autoprefixer": "^10",
        "dotenv": "^16.4.7",
        "eslint": "^8",
        "eslint-config-next": "13.5.4",
        "fast-check": "^3.13.2",
        "postcss": "^8",
        "tailwindcss": "^3",
        "typescript": "^5",
        "vitest": "^1.3.1"
    }
}