# .env.example
# Vercel Blob Storage
BLOB_READ_WRITE_TOKEN=

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# OpenAI API
OPENAI_API_KEY=
OPENAI_ORGANIZATION_ID=

# Next.js Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Feature Flags
ENABLE_PROCESSING=true
ENABLE_CACHING=true
DEBUG_MODE=false

# Authentication (if needed later)
# NEXTAUTH_URL=http://localhost:3000
# NEXTAUTH_SECRET=

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW_MS=60000

# Cron Jobs
CRON_SECRET=

# .env.development
# This will be used for local development
# You should create this file manually and not commit it to git

# .env.production
# This would be used in production environments
# Do not create this file locally, use Vercel environment variables instead

# .env.test
# This would be used for testing environments